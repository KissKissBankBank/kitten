    <%= example 'Horizontal Nav' do %>
      <%= react_component 'KarlHorizontalNav' %>
    <% end %>
  </div>

  <div class="k-Grid__col k-Grid__col--12">
    <%= example 'Horizontal Nav touch' do %>
      <%= react_component 'KarlTouchHorizontalNav' %>
