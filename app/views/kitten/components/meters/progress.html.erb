<div class="k-Grid">
  <div class="k-Grid__col k-Grid__col--12 k-Grid__col--6@l">
    <div class="k-Grid">
      <div class="k-Grid__col k-Grid__col--12 k-Grid__col--4@m k-Grid__col--12@l">
        <%= example 'Normal' do %>
          <%= react_component('Progress', props: {
              value: 60
            }
          ) %>
        <% end %>
      </div>

      <div class="k-Grid__col k-Grid__col--12 k-Grid__col--4@m k-Grid__col--12@l">
        <%= example 'With animation' do %>
          <%= react_component('KarlRandomProgress') %>
        <% end %>
      </div>
    </div>
  </div>
</div>
