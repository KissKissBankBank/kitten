<div class="k-Grid">
  <div class="k-Grid__col k-Grid__col--12 k-Grid__col--3@m">
    <%= example 'Article card' do %>
      <%= react_component 'ArticleCard' %>
    <% end %>
  </div>

  <div class="k-Grid__col k-Grid__col--12 k-Grid__col--3@m">
    <%= example 'Clickable article card' do %>
      <%= react_component 'ArticleCard', props: {
          href: '#',
        } %>
    <% end %>
  </div>
</div>
