<%= example "button tag" do %>
  <button class="k-ButtonImage" title="Profil">
    <img class="k-ButtonImage__img"
      src="https://unsplash.it/40/40?image=839"
         alt=""
         width="40"
         height="40" />
  </button>
<% end %>

<%= example "a tag" do %>
  <a href="#" class="k-ButtonImage" title="Profil">
    <img class="k-ButtonImage__img"
      src="https://unsplash.it/40/40?image=836" width="40" height="40" />
  </a>
<% end %>

