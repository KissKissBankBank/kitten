{
  "name": "kitten-components",
  "version": "23.16.1",
  "license": "MIT",
  "description": "Front-end components library",
  "main": "src",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KissKissBankBank/kitten.git"
  },
  "keywords": [
    "sass",
    "scss",
    "react",
    "components",
    "kitten"
  ],
  "author": "KissKissBankBank & Co",
  "homepage": "https://github.com/KissKissBankBank/kitten#readme",
  "dependencies": {
    "bowser": "1.9.4",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "event-emitter": "^0.3.4",
    "html-to-react": "^1.3.2",
    "is-string-a-number": "2.0.2",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^22.0.4",
    "json-loader": "^0.5.4",
    "modularscale-sass": "^2.1.1",
    "node-sass": "^3.8.0",
    "prop-types": "^15.6.0",
    "prop-types-extra": "^1.0.1",
    "pubsub-js": "^1.5.3",
    "radium": "^0.25.0",
    "react": "^16.2.0",
    "react-ckeditor-wrapper": "^1.0.22",
    "react-cropper": "^0.12.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^3.13.1",
    "react-markdown": "^2.4.4",
    "react-masonry-component": "6.0.2",
    "react-modal": "3.5.1",
    "react-places-autocomplete": "^5.4.3",
    "react-select": "^1.0.0",
    "react-test-renderer": "^16.2.0",
    "react-tooltip": "^3.2.2",
    "react-truncate": "^2.3.0",
    "resize-observer-polyfill": "^1.5.0",
    "smoothscroll-polyfill": "^0.4.0",
    "susy": "2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-react-jsx": "7.1.6",
    "@babel/preset-env": "7.1.6",
    "@storybook/addon-a11y": "4.0.7",
    "@storybook/addon-actions": "4.0.7",
    "@storybook/addon-info": "4.0.7",
    "@storybook/addon-knobs": "4.0.7",
    "@storybook/addon-links": "4.0.7",
    "@storybook/addon-viewport": "4.0.7",
    "@storybook/addons": "4.0.7",
    "@storybook/react": "4.0.7",
    "@storybook/storybook-deployer": "2.3.0",
    "autoprefixer": "9.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "chai": "4.2.0",
    "chai-enzyme": "0.8.0",
    "css-loader": "1.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.4",
    "file-loader": "2.0.0",
    "htmlprocessor": "0.2.6",
    "husky": "1.1.4",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jsdom": "13.0.0",
    "json-loader": "^0.5.4",
    "lint-staged": "8.0.5",
    "mini-css-extract-plugin": "0.4.4",
    "mocha": "5.2.0",
    "node-sass": "4.10.0",
    "prettier": "1.15.2",
    "react-scrollable-anchor": "0.6.1",
    "sass-loader": "7.1.0",
    "sinon": "7.1.1",
    "sinon-test": "2.4.0",
    "style-loader": "0.23.1",
    "stylelint": "^6.7.1",
    "stylelint-scss": "^1.2.0",
    "webpack": "4.26.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "scripts": {
    "stylelint": "stylelint --syntax scss 'assets/stylesheets/**/*.scss'",
    "test": "jest --maxWorkers=2",
    "test:update:snapshots": "jest --updateSnapshot",
    "precommit": "lint-staged",
    "styleguide:start": "webpack-dev-server",
    "styleguide:build": "rm -rf styleguide/build && webpack --config webpack.config.build.js --progress --colors && htmlprocessor styleguide/index.html -o styleguide/build/index.html && cp -R styleguide/assets styleguide/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "build": "build:js && build:css",
    "build:js": "babel assets/javascripts/kitten/ -d src --ignore 'assets/**/*.test.js','assets/**/__mocks__/*.js'",
    "build:css": "webpack --config webpack.config.sass.js"
  },
  "lint-staged": {
    "assets/**/*.js": [
      "prettier --single-quote --no-semi --trailing-comma all --write",
      "git add"
    ]
  },
  "jest": {
    "testURL": "http://localhost/",
    "modulePaths": [
      "assets/javascripts"
    ],
    "setupFiles": [
      "./assets/javascripts/kitten/config/jest-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./assets/javascripts/kitten/config/__mocks__/fileMock.js",
      "\\.(css|less)$": "./assets/javascripts/kitten/config/__mocks__/styleMock.js"
    }
  }
}
