{
  "name": "@kisskissbankbank/kitten",
  "version": "3.18.1",
  "license": "MIT",
  "description": "Front-end components library",
  "main": "src",
  "module": "src/esm",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KissKissBankBank/kitten.git"
  },
  "engines": {
    "node": ">=12.x"
  },
  "keywords": [
